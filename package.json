    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "antd": "^4.15.2",
    "clsx": "^1.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fast-deep-equal": "^3.1.3",
    "query-string": "^7.0.0",
    "random-js": "^2.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "unchanged": "^2.2.1",
    "unfetch": "^4.1.0"
  }
}
