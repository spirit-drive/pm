{
  "name": "test",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "sideEffects": [
    "**/*.sass"
  ],
  "scripts": {
    "start": "node server.js",
    "dev": "webpack-dev-server --mode development",
    "lint": "eslint --fix src",
    "build": "webpack --mode production",
    "test": "jest src",
    "cypress": "./node_modules/.bin/cypress open",
    "schema": "node ./src/Client/schemaQuery.js",
    "docs": "node_modules/.bin/jsdoc -c jsdoc.json",
    "push-heroku": "git push heroku master",
    "nominals": "ts-node -P ./src/core/nominals/tsconfig.json ./src/core/nominals/writeAllNominals.ts",
    "possible-chains": "ts-node -P ./src/core/nominals/tsconfig.json ./src/core/nominals/writeAllPossibleChains.ts",
    "explore": "ts-node -P ./src/core/explore/tsconfig.json ./src/core/explore/explore.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{*.tsx,*.ts,*.js}": [
      "eslint --fix src",
      "prettier --write src",
      "jest src"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "modulePaths": [
      "src"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "\\.sass$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "private": true,
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^1.3.1",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@babel/preset-typescript": "^7.9.0",
    "@types/apollo-upload-client": "^8.1.3",
    "@types/node": "^13.9.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "autoprefixer": "^10.4.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-preset-react": "^6.24.1",
    "buffer": "^6.0.3",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.7.1",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^5.1.7",
    "cypress": "^4.2.0",
    "cypress-plugin-tab": "^1.0.5",
    "docdash": "^1.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.0.0",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "img-loader": "^3.0.1",
    "jest": "^25.1.0",
    "jsdoc": "^3.6.3",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^10.0.8",
    "mini-css-extract-plugin": "2.4.5",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.2",
    "react-styleguidist": "^11.0.4",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.51.0",
