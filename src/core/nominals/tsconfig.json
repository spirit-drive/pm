{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "noImplicitAny": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "moduleResolution": "node",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "es5", "scripthost", "es2015.promise"]
  }
}
